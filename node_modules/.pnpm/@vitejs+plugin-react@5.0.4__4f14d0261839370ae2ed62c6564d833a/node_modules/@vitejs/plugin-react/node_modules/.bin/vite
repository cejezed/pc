#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/PersonalCoach/node_modules/.pnpm/vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules/vite/bin/node_modules:/mnt/e/PersonalCoach/node_modules/.pnpm/vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules/vite/node_modules:/mnt/e/PersonalCoach/node_modules/.pnpm/vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules:/mnt/e/PersonalCoach/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/PersonalCoach/node_modules/.pnpm/vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules/vite/bin/node_modules:/mnt/e/PersonalCoach/node_modules/.pnpm/vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules/vite/node_modules:/mnt/e/PersonalCoach/node_modules/.pnpm/vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules:/mnt/e/PersonalCoach/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@7.1.7_@types+node@20.1_04aa96cde21fe59f71bf28159510dc98/node_modules/vite/bin/vite.js" "$@"
fi
