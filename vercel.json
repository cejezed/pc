{
  "buildCommand": "npm --workspace=client run build",
  "installCommand": "npm install",
  "outputDirectory": "apps/client/dist",
  "framework": null,
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/meal-plans/generate-shopping-list",
      "destination": "/api/meal-plans/generate-shopping-list.ts"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
