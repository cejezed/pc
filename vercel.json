{
  "version": 2,
  "buildCommand": "npm --workspace=client run build",
  "devCommand": "npm --workspace=client run dev",
  "framework": "vite",
  "functions": {
    "api/**/*.{ts,js}": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}